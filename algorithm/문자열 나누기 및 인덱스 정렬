var len = ["try", "hello", "world"]

for (i, j) in len.enumerated() {

  print(i,j)
                     
  for (a,b) in j.enumerated() {
                
      print(a,b)
                      
  }      

}


// print(i,j)
0 try
1 hello
2 world

// print(a,b)
0 t
1 r
2 y
0 h
1 e
2 l
3 l
4 o
0 w
1 o
2 r
3 l
4 d
